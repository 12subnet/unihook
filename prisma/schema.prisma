generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["deno"]
  output          = "../generated/client"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Pattern {
  id      String  @id @map("_id")
  pattern String
  User    User?   @relation(fields: [userId], references: [id])
  userId  BigInt?
}

model User {
  id       BigInt    @id @map("_id")
  patterns Pattern[]
}
